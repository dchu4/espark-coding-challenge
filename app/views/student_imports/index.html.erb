<div class="container">
  <h1 class="text-center">Personalized Curriculum Imports</h1>
  <div class="row table-responsive">
    <div class="col-sm-12 col-md-6 col-lg-6">
      <table class="table-bordered table-striped">
        <% @student_imports.each do |student_import| %>
          <tr>
            <td><%= "Student Import #{student_import.id}" %></td>
            <td><%= link_to 'Show', "/personalized_curriculums/#{student_import.id}" %></td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>