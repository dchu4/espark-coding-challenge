<div class="container-fluid">
  <h1>Import Domain Order/Student Tests</h1>
  <div class="row">
    <div class="col-sm-12 col-md-6 col-lg-6 col-md-offset-3">
        <%= form_tag import_student_imports_path, multipart: true do %>
          <%= label_tag :domain_order %>
          <%= file_field_tag :domain_order %>

          <br>

          <%= label_tag :student_tests %>
          <%= file_field_tag :student_tests %>

          <br>

          <%= submit_tag "Import" %>
        <% end %>
    </div>
  </div>
</div>